{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SecuritySettings": {
    "PublicKey": "",
    "PrivateKey": "YOUR_PRIVATE_KEY_HERE"
